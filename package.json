{
  "name": "krabi",
  "description": "Premium Ghost Theme",
  "demo": "https://krabi.aspirethemes.com/",
  "version": "1.1.4",
  "engines": {
    "ghost": ">=4.0.0",
    "ghost-api": "v4"
  },
  "keywords": [
    "ghost-theme"
  ],
  "scripts": {
    "postinstall": "bower install"
  },
  "dependencies": {
    "@primer/css": "^17.11.1",
    "clipboard": "^2.0.8",
    "concat": "^3.0.0",
    "evil-icons": "^1.10.1",
    "fs": "0.0.2",
    "lazysizes": "^5.3.2",
    "lunr": "^0.7.2",
    "lunr-languages": "^1.9.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.25.0",
    "sass-mq": "^5.0.1",
    "tocbot": "^4.13.4"
  },
  "devDependencies": {
    "ansi-colors": "^1.1.0",
    "beeper": "^1.1.1",
    "dart-sass": "^1.25.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-jshint": "^2.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rtlcss": "^1.4.1",
    "gulp-sass": "^5.0.0",
    "gulp-size": "^4.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^4.2.0",
    "jshint": "^2.13.1",
    "jshint-stylish": "^2.2.1",
    "node-sass": "^6.0.1",
    "release-it": "^14.11.6",
    "run-sequence": "^2.2.1",
    "sass": "^1.35.1"
  },
  "author": {
    "name": "Aspire Themes",
    "email": "ahmad@aspirethemes.com",
    "url": "https://aspirethemes.com/"
  },
  "config": {
    "custom": {
      "show_scroll_to_top_button": {
        "type": "boolean",
        "default": false
      },
      "show_table_of_contents_on_all_posts": {
        "type": "boolean",
        "default": false,
        "group": "post"
      },
      "show_table_of_contents_on_all_pages": {
        "type": "boolean",
        "default": false,
        "group": "post"
      }
    },
    "posts_per_page": 8,
    "image_sizes": {
      "profile-image-small": {
        "width": 128
      },
      "profile-image-medium": {
        "width": 256
      },
      "profile-image-large": {
        "width": 320
      },
      "feature-image-small": {
        "width": 346
      },
      "feature-image-medium": {
        "width": 692
      },
      "feature-image-large": {
        "width": 1384
      }
    }
  }
}
